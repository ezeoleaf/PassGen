<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ezeoleaf.passgenerator" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PassGenerator</name>
    <description>
        A simple application to generate and store secure and randoms passwords.
    </description>
    <author email="eze.olea.f@gmail.com" href="https://github.com/ezeoleaf">
        Ezequiel Olea Figueroa
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="14" />
</widget>
